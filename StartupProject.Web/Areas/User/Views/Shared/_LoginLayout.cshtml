<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewData["Title"] - DailyStandup</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/png" href="~/lib/srtdash/images/icon/favicon.ico">
    <link rel="stylesheet" href="~/lib/srtdash/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/lib/srtdash/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/lib/srtdash/css/themify-icons.css">
    <link rel="stylesheet" href="~/lib/srtdash/css/metisMenu.css">
    <link rel="stylesheet" href="~/lib/srtdash/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/lib/srtdash/css/slicknav.min.css">
    <link href="~/lib/toastr/toastr.min.css" rel="stylesheet" />
    <!-- amchart css -->
    <link rel="stylesheet" href="https://www.amcharts.com/lib/3/plugins/export/export.css" type="text/css" media="all" />
    <!-- others css -->
    <link rel="stylesheet" href="~/lib/srtdash/css/typography.css">
    <link rel="stylesheet" href="~/lib/srtdash/css/default-css.css">
    <link rel="stylesheet" href="~/lib/srtdash/css/styles.css">
    <link rel="stylesheet" href="~/lib/srtdash/css/responsive.css">
    <!-- modernizr css -->
    <script src="~/lib/srtdash/js/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body>
    <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
    <!-- preloader area start -->
    <div id="preloader">
        <div class="loader"></div>
    </div>
    <!-- preloader area end -->
    <!-- login area start -->
    <div class="login-area">
        <div class="container">
            <div class="login-box ptb--100">
                @RenderBody()
            </div>
        </div>
    </div>
    <!-- login area end -->
    <!-- jquery latest version -->
    <script src="~/lib/srtdash/js/vendor/jquery-2.2.4.min.js"></script>
    <!-- bootstrap 4 js -->
    <script src="~/lib/srtdash/js/popper.min.js"></script>
    <script src="~/lib/srtdash/js/bootstrap.min.js"></script>
    <script src="~/lib/srtdash/js/owl.carousel.min.js"></script>
    <script src="~/lib/srtdash/js/metisMenu.min.js"></script>
    <script src="~/lib/srtdash/js/jquery.slimscroll.min.js"></script>
    <script src="~/lib/srtdash/js/jquery.slicknav.min.js"></script>
    <script src="~/lib/unobtrusive-ajax/unobtrusive-ajax.min.js"></script>
    <script src="~/lib/toastr/toastr.min.js"></script>

    <!-- others plugins -->
    <script src="~/lib/srtdash/js/plugins.js"></script>
    <script src="~/lib/srtdash/js/scripts.js"></script>
    <script src="~/js/GlobalFunction.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
